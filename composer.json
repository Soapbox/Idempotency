{
    "name": "soapbox/idempotency",
    "type": "library",
    "description": "A Laravel library that manages idempotency keys for a request",
    "license": "MIT",
    "require": {
        "ramsey/uuid": "^4.0",
        "guzzlehttp/psr7": "^1.4",
        "illuminate/support": "^10.48.11"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Soapbox/fake-requests"
        }
    ],
    "autoload": {
        "psr-4": {
            "SoapBox\\Idempotency\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SoapBox\\Idempotency\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5.20",
        "orchestra/testbench": "^8.23.0",
        "jshayes/fake-requests": "dev-php81"
    },
    "extra": {
        "laravel": {
            "providers": [
                "SoapBox\\Idempotency\\ServiceProvider"
            ]
        }
    }
}
